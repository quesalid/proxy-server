{
  "name": "proxy-server",
  "version": "1.0.0",
  "description": "Proxy Server",
  "main": "appserver.js",
  "type": "module",
  "scripts": {
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules NODE_TLS_REJECT_UNAUTHORIZED=0 jest --testTimeout=10000 --detectOpenHandles",
    "single": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules NODE_TLS_REJECT_UNAUTHORIZED=0 node test/singletest.js",
    "pretty": "npx prettier --write .",
    "stress": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 artillery run test/conf/test-artillery.yml --output test/conf/stresstest.json",
    "start": "node appserv.js",
    "testprod": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --testTimeout=10000 --detectOpenHandles",
    "testsingle": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules NODE_TLS_REJECT_UNAUTHORIZED=0 node test/singletest.js"
  },
  "author": "P. Pulicani",
  "license": "ISC",
  "dependencies": {
    "client-oauth2": "^4.3.3",
    "cluster": "^0.7.7",
    "codice-fiscale-js": "^2.3.20",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "express-sse": "^0.5.3",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "https": "^1.0.0",
    "jest": "^29.5.0",
    "jsonwebtoken": "^9.0.0",
    "memorystore": "^1.6.7",
    "minimist": "^1.2.8",
    "morgan": "^1.10.0",
    "node-fetch": "^3.3.1",
    "os": "^0.1.2",
    "papaparse": "^5.4.0",
    "socket.io-client": "^4.7.2",
    "uuid": "^9.0.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "isomorphic-unfetch": "^4.0.2",
    "jest-html-reporters": "^3.1.4",
    "prettier": "2.8.7",
    "supertest": "^6.3.3"
  }
}
